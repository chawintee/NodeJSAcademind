<%-include('includes/head.ejs')%>
    <link rel="stylesheet" href="/CSS/users.css">
</head>
<body>
    <%-include('includes/navigation.ejs')%>
    <main>
        <% if(users.length > 0) { %>
        <div>
            <h1>Users list</h1>
            <%for (let user of users) {%>
            <ul>
                <li><%= user.userName%></li>
            </ul>
            <% }%>
        </div>
        <% } else {%>
            <h1>No users!!!</h1>
        <% }%>
    </main>
<%-include('includes/end.ejs')%>
